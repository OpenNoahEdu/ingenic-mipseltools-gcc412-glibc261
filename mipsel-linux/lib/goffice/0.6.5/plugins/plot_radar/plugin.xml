<?xml version="1.0" encoding="UTF-8"?>
<plugin id="GOffice_plot_radar">
	<information>
		<name>Charting : Polar/Radar</name>
		<name xml:lang="ca">Gràfic: polar/radar</name>
		<name xml:lang="es">Gráfico: Polar/Radar</name>
		<name xml:lang="fr">Graphe : Polaire/Radar</name>
		<name xml:lang="it">Grafici: polare/rete</name>
		<name xml:lang="ja">グラフ: 極座標/レーダー</name>
		<name xml:lang="vi">Vẽ đồ thị: Cực/Ra đa</name>
		<description>Polar and Radar plots</description>
		<description xml:lang="ca">Traçats polars i de radar</description>
		<description xml:lang="es">Dibujos polar y de radar</description>
		<description xml:lang="fr">Tracés polaires et en radar.</description>
		<description xml:lang="it">Tracciati polari e rete</description>
		<description xml:lang="vi">Đồ thị vẽ Cực và Ra đa</description>
	</information>
	<loader type="Gnumeric_Builtin:module">
		<attribute value="radar" name="module_file"/>
	</loader>
	<services>
		<service type="plot_engine" id="GogRadarPlot">
			<information>
				<description>Radar plotting engine</description>
				<description xml:lang="ca">Motor de traçador de radar</description>
				<description xml:lang="cs">Engine kreslení radarů</description>
				<description xml:lang="de">Radargraph-Engine</description>
				<description xml:lang="en_CA">Radar plotting engine</description>
				<description xml:lang="en_GB">Radar plotting engine</description>
				<description xml:lang="es">Motor de dibujo de radares</description>
				<description xml:lang="fr">Moteur de dessin de radar</description>
				<description xml:lang="it">Motore tracciati rete</description>
				<description xml:lang="ja">レーダーグラフプロットエンジン</description>
				<description xml:lang="nl">Radar plot-motor</description>
				<description xml:lang="pt_BR">Mecanismo de plotar gráficos de Radar</description>
				<description xml:lang="sv">Radardiagramsritmotor</description>
				<description xml:lang="vi">Cơ chế vẽ ra đa</description>
			</information>
		</service>
		<service type="plot_engine" id="GogRadarAreaPlot">
			<information>
				<description>Radar Area plotting engine</description>
				<description xml:lang="ca">Motor de traçador d'àrees de radar</description>
				<description xml:lang="cs">Engine kreslení oblastí radaru</description>
				<description xml:lang="de">Radarflächengraph-Engine</description>
				<description xml:lang="en_CA">Radar Area plotting engine</description>
				<description xml:lang="en_GB">Radar Area plotting engine</description>
				<description xml:lang="es">Motor de dibujo de áreas de radar</description>
				<description xml:lang="fr">Moteur de dessin de radar</description>
				<description xml:lang="it">Motore traciati rete area</description>
				<description xml:lang="nl">Radargebied plot-motor</description>
				<description xml:lang="pt_BR">Mecanismo de plotagem de gráficos de Radar de Área</description>
				<description xml:lang="sv">Ytradardiagramsritmotor</description>
				<description xml:lang="vi">Cơ chế vẽ vùng ra đa</description>
			</information>
		</service>
		<service type="plot_engine" id="GogPolarPlot">
			<information>
				<description>Polar plotting engine</description>
				<description xml:lang="ca">Motor de traçador polar</description>
				<description xml:lang="es">Motor de dibujo polar</description>
				<description xml:lang="fr">Moteur de dessin polaire</description>
				<description xml:lang="it">Motore tracciati polari</description>
				<description xml:lang="ja">極座標グラフプロットエンジン</description>
				<description xml:lang="vi">Cơ chế vẽ cực</description>
			</information>
		</service>
		<service type="plot_type" id="radar">
			<file>plot-types.xml</file>
			<information>
				<description>Default radar plot types</description>
				<description xml:lang="ca">Tipus de traçat de radar per defecte</description>
				<description xml:lang="cs">Implicitní typy radarových grafů</description>
				<description xml:lang="de">Vorgabe-Radargraphtypen</description>
				<description xml:lang="en_CA">Default radar plot types</description>
				<description xml:lang="en_GB">Default radar plot types</description>
				<description xml:lang="es">Tipos de gráficas de radar preestablecidas</description>
				<description xml:lang="fr">Type de dessin polaire/radar par défaut</description>
				<description xml:lang="it">Tipi predefiniti grafici rete</description>
				<description xml:lang="ja">デフォルトのレーダーグラフ形式</description>
				<description xml:lang="nl">Standaard radargrafiek-types</description>
				<description xml:lang="pt_BR">Tipos padrão de gráfico de radar</description>
				<description xml:lang="sv">Standard radargraftyper</description>
				<description xml:lang="vi">Kiểu đồ thị vẽ ra đa mặc định</description>
			</information>
		</service>
	</services>
</plugin>